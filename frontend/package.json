    "@types/nprogress": "^0.2.0",
    "@types/utf8": "^3.0.1",
    "@wangeditor/editor": "^5.1.14",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "animejs": "^3.2.1",
    "axios": "^0.27.2",
    "base64-arraybuffer": "^1.0.2",
    "echarts": "^5.4.1",
    "element-plus": "^2.2.6",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "json-as-xlsx": "^2.5.3",
    "luxon": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "pinia-plugin-persist": "^1.0.0",
    "sass": "^1.75.0",
    "sass-loader": "^13.0.0",
    "utf8": "^3.0.0",
    "vue": "^3.2.36",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/animejs": "^3.1.4",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.36",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^10.0.2",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
